package cz.epickejtejpek.kalkulacka;

public class Enumeratory {
 
	//JEDNÁ SE O OSEKANÝ/ VLASTNÍ DATOVÝ TYP
	
	enum Barvy { CERVENA, ZELENA, CERNA, MODRA, ZLUTA	}
	
	public static void main(String[] args) {
		Barvy barvicka = Barvy.CERNA;	// používáno v if. Omezení programatorovy aby nepoužil barvy které nejsou v programu

		if(barvicka == Barvy.CERNA) {
			System.out.println("Mám rád černou");
		}
	}

}
 